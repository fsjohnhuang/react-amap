{
  "name": "react-amap2",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lerna": "lerna",
    "build": "lerna exec \"tsbb build\"",
    "clear": "lerna exec \"rimraf esm cjs\"",
    "watch:require-script": "lerna exec \"tsbb watch\" --scope @react-amap2/require-script",
    "watch:utils": "lerna exec \"tsbb watch\" --scope @react-amap2/utils",
    "watch:script-loader": "lerna exec \"tsbb watch\" --scope @react-amap2/script-loader",
    "watch:api-loader": "lerna exec \"tsbb watch\" --scope @react-amap2/api-loader"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lerna": "^6.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsbb": "~3.7.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
